{
  "name": "sapientpro/phasher",
  "description": "Library for image comparison",
  "require": {
    "php": "^8.1",
    "ext-gd": "*"
  },
  "autoload": {
    "psr-4": {
      "SapientPro\\PHasher\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SapientPro\\PHasher\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "10.0"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs .",
    "tests-unit": "vendor/bin/phpunit --testsuite=unit"
  }
}
